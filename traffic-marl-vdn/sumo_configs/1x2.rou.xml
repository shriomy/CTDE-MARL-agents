<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    
    <!-- Define vehicle types -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5.0" maxSpeed="50.0"/>
    
    <!-- Define routes -->
    <route id="route1" edges="e1 e2"/>
    <route id="route2" edges="e3 e4"/>
    
    <!-- Generate vehicles -->
    <flow id="flow1" type="car" route="route1" begin="0" end="3600" period="3"/>
    <flow id="flow2" type="car" route="route2" begin="0" end="3600" period="5"/>
    
</routes>